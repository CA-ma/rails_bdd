<!DOCTYPE html>
<html>
  <head>
    <title>RailsDemo</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= notice || alert %>
    <h3> <%= link_to 'Home', root_path %> &nbsp; <%= link_to 'New Post', new_article_path %> &nbsp; <%= link_to 'Login', new_user_session_path %> &nbsp; <%= link_to 'Logout', destroy_user_session_path, method: :delete %> &nbsp; <%= link_to 'Sign Up', new_user_registration_path %> </h3>
    <h3> </h3>
    <hr></hr>
    <%= yield %>
  </body>
</html>
