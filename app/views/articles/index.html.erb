<h3> <%= link_to 'Articles', root_path %> -- <%= link_to 'Post new article', new_article_path %> </h3>
<hr></hr>
<h1>Articles</h1>
<h3>Find all the latests posts right here.</h3>


<% @articles.each do |article| %>
    <h3><%=article.id%>.  <%= link_to article.title, article_path(article) %></h3>
    <p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= article.content %></p>
<% end %>
