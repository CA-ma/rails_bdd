<h3> <%= link_to 'Articles', root_path %> -- <%= link_to 'Post new article', new_article_path %> </h3>
<hr></hr>
<h1>Articles</h1>
<h3>Find all the latests posts right here.</h3>
<hr></hr>

<% @articles.each do |article| %>
    <h3>Article <%=article.id%> -- <%= link_to 'Show', article_path(article) %> -- <%= link_to 'Edit', edit_article_path(article)%></h3>
    <p> <strong><%=article.title%></strong> </p>
    <p><%= article.content %></p>
<% end %>
